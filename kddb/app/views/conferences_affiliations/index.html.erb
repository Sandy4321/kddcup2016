<p id="notice"><%= notice %></p>

<h1>Listing Conferences Affiliations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conferences_affiliations.each do |conferences_affiliation| %>
      <tr>
        <td><%= link_to 'Show', conferences_affiliation %></td>
        <td><%= link_to 'Edit', edit_conferences_affiliation_path(conferences_affiliation) %></td>
        <td><%= link_to 'Destroy', conferences_affiliation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Conferences affiliation', new_conferences_affiliation_path %>
